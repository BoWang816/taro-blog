{
    "name": "taro-blog",
    "version": "0.0.1",
    "private": true,
    "description": "小程序博客，基于Taro，同步hexo博客数据",
    "templateInfo": {
        "name": "mobx",
        "typescript": false,
        "css": "less"
    },
    "scripts": {
        "build:weapp": "taro build --type weapp",
        "build:swan": "taro build --type swan",
        "build:alipay": "taro build --type alipay",
        "build:tt": "taro build --type tt",
        "build:h5": "taro build --type h5",
        "build:rn": "taro build --type rn",
        "build:qq": "taro build --type qq",
        "build:jd": "taro build --type jd",
        "build:quickapp": "taro build --type quickapp",
        "dev:weapp": "npm run build:weapp -- --watch",
        "dev:swan": "npm run build:swan -- --watch",
        "dev:alipay": "npm run build:alipay -- --watch",
        "dev:tt": "npm run build:tt -- --watch",
        "dev:h5": "npm run build:h5 -- --watch",
        "dev:rn": "npm run build:rn -- --watch",
        "dev:qq": "npm run build:qq -- --watch",
        "dev:jd": "npm run build:jd -- --watch",
        "dev:quickapp": "npm run build:quickapp -- --watch"
    },
    "browserslist": [
        "last 3 versions",
        "Android >= 4.1",
        "ios >= 8"
    ],
    "author": "bo.wang",
    "license": "MIT",
    "dependencies": {
        "@babel/runtime": "^7.7.7",
        "@tarojs/runtime": "3.0.24",
        "@tarojs/taro": "3.0.24",
        "@tarojs/components": "3.0.24",
        "classnames": "^2.2.6",
        "mobx": "^4.8.0",
        "mobx-react": "^6.1.4",
        "react-dom": "^16.10.0",
        "@tarojs/react": "3.0.24",
        "react": "^16.10.0",
        "taro-axios": "^1.1.1"
    },
    "devDependencies": {
        "@babel/core": "^7.12.10",
        "@babel/plugin-proposal-class-properties": "^7.10.4",
        "@babel/plugin-proposal-decorators": "^7.10.5",
        "@babel/plugin-proposal-object-rest-spread": "^7.11.0",
        "@babel/plugin-transform-arrow-functions": "^7.10.4",
        "@babel/plugin-transform-runtime": "^7.11.0",
        "@babel/preset-env": "^7.12.11",
        "@tarojs/cli": "^3.0.8",
        "@tarojs/mini-runner": "3.0.24",
        "@tarojs/webpack-runner": "3.0.24",
        "@types/react": "^16.0.0",
        "@types/webpack-env": "^1.13.6",
        "babel-eslint": "^10.1.0",
        "babel-loader": "^8.2.2",
        "babel-preset-taro": "3.0.24",
        "chalk": "^4.1.0",
        "dayjs": "^1.8.33",
        "eslint": "^6.8.0",
        "eslint-config-airbnb": "^18.2.0",
        "eslint-config-prettier": "^6.11.0",
        "eslint-config-taro": "3.0.24",
        "eslint-import-resolver-alias": "^1.1.2",
        "eslint-import-resolver-webpack": "^0.13.0",
        "eslint-plugin-import": "^2.12.0",
        "eslint-plugin-jsx-a11y": "^6.3.1",
        "eslint-plugin-prettier": "^3.1.4",
        "eslint-plugin-react": "^7.8.2",
        "eslint-plugin-react-hooks": "^1.6.1",
        "husky": "^4.2.5",
        "lint-staged": "^10.2.13",
        "minimist": "^1.2.5",
        "prettier": "^2.1.1",
        "pretty-quick": "^3.0.0",
        "stylelint": "9.3.0",
        "webpack-bundle-analyzer": "^3.6.0"
    },
    "husky": {
        "hooks": {
            "pre-commit": "pretty-quick --staged && lint-staged",
            "post-commmit": "git update-index --again"
        }
    },
    "lint-staged": {
        "src/**/*.{js,less}": "git add"
    }
}
